<!-- Navigation -->
<%= render 'layouts/navbar' %>

<!-- Header -->
<header class="masthead">
  <div class="container">
    <%= image_tag "flower_hanataba_bara_red.png", class: "img-fluid", :size => "300x300" %>
    <div class="intro-text">
      <span class="name">ポプラー(pop_flower)</span>
      <hr class="star-light">
      <span class="skills">ポプラー(pop_flower)とは、大切な日に贈りたい花を投票するサイトです。</span>
    </div>
  </div>
</header>

<!-- Portfolio Grid Section -->
<section id="portfolio">
  <div class="container">
    <h2 class="text-center">大切な日</h2>
    <hr class="star-primary">
    <div class="row">
      <% @event.each do | event | %>
          <div class="col-sm-4 portfolio-item">
            <p><%= event.title %></p>
            <a class="portfolio-link" href="/events/<%= event.id %>">
              <div class="caption">
                <div class="caption-content">
                  <i class="fa fa-search-plus fa-3x"></i>
                </div>
              </div>
              <%= image_tag "event/#{event.image}.png", class: "img-fluid" %>
            </a>
          </div>
      <% end %>
    </div>
  </div>
</section>

<!-- About Section -->
<%= render 'layouts/about' %>
<!-- Contact Section -->
<!-- Footer -->
<%= render 'layouts/footer' %>

<!-- Scroll to Top Button (Only visible on small and extra-small screen sizes) -->
<div class="scroll-top hidden-lg-up">
  <a class="btn btn-primary page-scroll" href="#page-top">
    <i class="fa fa-chevron-up"></i>
  </a>
</div>

